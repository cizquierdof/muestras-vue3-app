{
  "private": true,
  "packageManager": "pnpm@7.1.6",
  "scripts": {
    "lint": "eslint .",
    "lint:ci": "eslint . --format junit -o lint/lintReport.xml",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "test:unit": "vitest --run",
    "test:integration": "cypress open -C cypressE2E.json",
    "test:e2e": "cypress open ",
    "test:and:coverage": "vitest run --coverage --reporter junit --outputFile unitTestReports/junitStyleReport.xml",
    "test:e2e:silent": "cypress run -C cypressE2E.json ",
    "test:e2e:silent:chrome": "cypress run -C cypressE2E.json --browser chrome ",
    "test:integration:silent": "cypress run -C cypressIntegration.json ",
    "dev": "vite --port 3333 --open",
    "dev:silent": "vite --port 3333",
    "build": "vite-ssg build",
    "build:noSSG": "vite build",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "start:crawler": "node src/helpers/server.js"
  },
  "dependencies": {
    "@axe-core/puppeteer": "^4.4.3",
    "@mdi/font": "^6.7.96",
    "@vueuse/core": "^8.5.0",
    "@vueuse/head": "^0.7.6",
    "console-stamp": "^3.0.6",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "glob": "^8.0.3",
    "nprogress": "^0.2.0",
    "path": "^0.12.7",
    "pinia": "^2.0.14",
    "playwright": "^1.22.2",
    "prism-theme-vars": "^0.2.3",
    "puppeteer": "13.0.0",
    "puppeteer-core": "^14.3.0",
    "vite-plugin-vuetify": "^1.0.0-alpha.11",
    "vue": "^3.2.36",
    "vue-demi": "^0.12.5",
    "vue-i18n": "^9.1.10",
    "vue-router": "^4.0.15",
    "vuetify": "3.0.0-beta.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.24.2",
    "@iconify-json/carbon": "^1.1.5",
    "@intlify/vite-plugin-vue-i18n": "^4.0.0",
    "@pinia/testing": "^0.0.12",
    "@types/markdown-it-link-attributes": "^3.0.1",
    "@types/nprogress": "^0.2.0",
    "@types/puppeteer": "^5.4.6",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/test-utils": "^2.0.0",
    "c8": "^7.11.3",
    "critters": "^0.0.16",
    "cross-env": "^7.0.3",
    "cypress": "^9.7.0",
    "eslint": "^8.16.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-security": "^1.5.0",
    "https-localhost": "^4.7.1",
    "markdown-it-link-attributes": "^4.0.0",
    "markdown-it-prism": "^2.2.4",
    "pnpm": "^7.1.6",
    "typescript": "^4.7.2",
    "unocss": "^0.34.1",
    "unplugin-auto-import": "^0.8.5",
    "unplugin-vue-components": "^0.19.6",
    "vite": "^2.9.9",
    "vite-plugin-inspect": "^0.5.0",
    "vite-plugin-md": "^0.13.1",
    "vite-plugin-pages": "^0.23.0",
